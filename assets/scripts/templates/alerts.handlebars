<div class="alert {{alertType}} alert-dismissable" role="alert">
  <div class="container-fluid">
    <div class="row">
      <div class="col-xs-12">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <p>{{message}}</p>
      </div>
    </div>
  </div>
</div>
